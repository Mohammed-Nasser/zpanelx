<div class="module-header">
  <h1 class="module-title">
    <img src="<@ ModuleIcon @>">
    <@ ModuleName @>
  </h1>
</div>

<div class="module-desc">
  <@ ModuleDesc @>
</div>

<div class="container-fluid module-content">

  <button class="btn" type="submit" onclick="window.open('etc/apps/phpinfo/')"><: View full PHP configuration :></button>
  <br><br>
  <div class="row-fluid" id="php-info">
   <@ PHPInfo @>
  </div>

</div>